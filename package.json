{
	"name": "electronapp",
	"author": "Adrian Torres",
	"description": "App La Guaira para Windows",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"main": "dist-electron/main.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:react dev:electron",
		"dev:react": "vite",
		"dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
		"build": "tsc -b && vite build",
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"preview": "vite preview",
		"transpile:electron": "tsc --project src/electron/tsconfig.json",
		"dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64"
	},
	"dependencies": {
		"@popperjs/core": "2.11.8",
		"@yudiel/react-qr-scanner": "2.0.8",
		"bootstrap": "5.3.3",
		"bootstrap-icons": "1.11.3",
		"datatables.net-bs5": "2.1.8",
		"datatables.net-buttons-bs5": "3.2.0",
		"datatables.net-react": "1.0.0",
		"jquery": "3.7.1",
		"leaflet": "1.9.4",
		"leaflet-routing-machine": "3.2.12",
		"pdfmake": "0.2.16",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-leaflet": "4.2.1",
		"react-router-dom": "6.28.0",
		"react-simple-keyboard": "3.8.36",
		"simple-keyboard-layouts": "3.4.53"
	},
	"devDependencies": {
		"@eslint/js": "9.18.0",
		"@types/bootstrap": "5.2.10",
		"@types/jquery": "3.5.32",
		"@types/leaflet": "1.9.14",
		"@types/leaflet-routing-machine": "3.2.8",
		"@types/pdfmake": "0.2.10",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"@typescript-eslint/parser": "^8.20.0",
		"@vitejs/plugin-react": "4.3.3",
		"autoprefixer": "10.4.20",
		"cross-env": "7.0.3",
		"electron": "33.2.0",
		"electron-builder": "25.1.8",
		"eslint": "9.18.0",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"npm-run-all": "4.1.5",
		"sass": "1.82.0",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.20.0",
		"vite": "5.4.10"
	}
}
